    "# 2) Factors that contribute to player points"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "fe8f9bb5",
   "metadata": {},
   "outputs": [],
   "source": [
    "#For entire seasons\n",
    "points = pd.DataFrame(cleanNBA['pts'])\n",
    "age = pd.DataFrame(cleanNBA['age'])\n",
    "net_rating = pd.DataFrame(cleanNBA['net_rating'])\n",
    "usage_rate = pd.DataFrame(cleanNBA['usg_pct'])\n",
    "games_played = pd.DataFrame(cleanNBA['gp'])"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "4ae1da9a",
   "metadata": {},
   "source": [
    "# ----------------------------------------------AGE----------------------------------------------------------"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "6fe0249b",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "LinearRegression()"
      ]
     },
     "execution_count": 11,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "from sklearn.model_selection import train_test_split\n",
    "\n",
    "from sklearn.linear_model import LinearRegression\n",
    "points_train, points_test, age_train, age_test = train_test_split(points, age, test_size=0.25)\n",
    "#print(\"Train Set :\", points_train.shape, age_train.shape)\n",
    "#print(\"Test Set  :\", points_test.shape, age_test.shape)\n",
    "\n",
    "linreg = LinearRegression()\n",
    "linreg.fit(age_train, points_train)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "b7a27e98",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Intercept \t: a =  [[-0.08504709]]\n",
      "Coefficients \t: b =  [13.11654621]\n"
     ]
    }
   ],
   "source": [
    "print('Intercept \\t: a = ', linreg.coef_)\n",
    "print('Coefficients \\t: b = ', linreg.intercept_)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "c1dfff92",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 1152x576 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "regline_x = age_train\n",
    "regline_y = linreg.intercept_ + linreg.coef_ * age_train\n",
    "\n",
    "f = plt.figure(figsize=(16, 8))\n",
    "plt.scatter(age_train, points_train)\n",
    "plt.plot(regline_x, regline_y, 'r-', linewidth = 3)\n",
    "plt.xlabel('Age', fontsize=20)\n",
    "plt.ylabel('Points Per Game', fontsize=20)\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "d1bbf43e",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Explained Variance for the train dataset (R^2): 0.004021246105354037\n",
      "Root Mean Squared Error for test dataset (RMSE): 5.738589397322291\n"
     ]
    }
   ],
   "source": [
    "points_test_pred = linreg.predict(age_test)\n",
    "print(\"Explained Variance for the train dataset (R^2):\", linreg.score(age_train, points_train))\n",
    "\n",
    "def mean_sq_err(actual, predicted):\n",
    "    return np.mean(np.square(np.array(actual) - np.array(predicted)))\n",
    "\n",
    "mseTest = mean_sq_err(points_test, points_test_pred)\n",
    "print(\"Root Mean Squared Error for test dataset (RMSE):\", np.sqrt(mseTest))"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "c7e5f3af",
